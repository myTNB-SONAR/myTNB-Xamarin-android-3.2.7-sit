<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/rootView"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:layout_alignParentBottom="true"
	android:layout_centerHorizontal="true"
	android:descendantFocusability="beforeDescendants"
	android:focusableInTouchMode="true"
	android:theme="@style/Theme.Dashboard"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/mainView"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:gravity="center"
		android:isScrollContainer="false"
		android:orientation="vertical">
		<include
			android:id="@+id/toolbar"
			layout="@layout/toolbar" />
		<ScrollView
			android:layout_width="match_parent"
			android:layout_weight="1"
			android:overScrollMode="never"
			android:scrollbars="none"
			android:fillViewport="true"
			android:id="@+id/myHomeScrollView"
			android:layout_height="wrap_content"
			android:background="@color/background_pale_grey">
			<LinearLayout
				android:orientation="vertical"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/myHomeScrollViewContainer">
				<LinearLayout
					android:orientation="vertical"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="@color/white"
					android:id="@+id/myHomeHeaderContainer"
					android:padding="16dp">
					<TextView
						android:id="@+id/txtMyHomeHeaderTitle"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:layout_gravity="center|left"
						android:text="Submit application for an electricity connection account."
						android:textColor="@color/tunaGrey"
						android:textSize="14dp" />
				</LinearLayout>
				<LinearLayout
					android:orientation="vertical"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="18dp"
					android:id="@+id/myHomeInfoContainer">
					<TextView
						android:id="@+id/txtMyHomeInfoTitle"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="16dp"
						android:layout_marginRight="16dp"
						android:text="I want to:"
						android:textAllCaps="false"
						android:textColor="@color/powerBlue"
						android:textSize="16dp" />
					<LinearLayout
						android:orientation="vertical"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="8dp"
						android:background="@android:color/transparent"
						android:id="@+id/myHomeInfoFieldContainer"
						android:paddingLeft="16dp"
						android:paddingRight="16dp">
						<androidx.constraintlayout.widget.ConstraintLayout
							android:id="@+id/myHomeConstraint"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:background="@color/white"
							android:orientation="vertical"
							android:paddingBottom="18dp"
							tools:layout_editor_absoluteX="0dp"
							tools:layout_editor_absoluteY="47dp">
							<ImageView
								android:id="@+id/ImageView_idFeedbackNewIC"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginLeft="16dp"
								android:layout_marginTop="20dp"
								android:src="@drawable/icon_move_in"
								app:layout_constraintLeft_toLeftOf="parent"
								app:layout_constraintTop_toTopOf="parent"
								app:srcCompat="@drawable/ic_feedback_bill" />
							<TextView
								android:id="@+id/txtViewid_FeedbackNewIC"
								android:layout_width="wrap_content"
								android:layout_height="20dp"
								android:layout_marginLeft="16dp"
								android:ellipsize="end"
								android:maxEms="15"
								android:maxLines="1"
								android:text="Move In"
								android:textColor="#49494A"
								android:textSize="14dp"
								app:layout_constraintLeft_toRightOf="@+id/ImageView_idFeedbackNewIC"
								android:layout_marginStart="16dp"
								app:layout_constraintTop_toTopOf="parent"
								android:layout_marginTop="16dp" />
							<TextView
								android:id="@+id/textviewid_subContent_FeedbackNewIC"
								android:layout_width="0dp"
								android:layout_height="wrap_content"
								android:layout_marginStart="16dp"
								android:layout_marginLeft="16dp"
								android:paddingRight="16dp"
								android:text="I want to open an account for new/existing premise."
								android:textColor="#8B8B8B"
								android:textSize="12dp"
								app:layout_constraintBottom_toBottomOf="parent"
								app:layout_constraintEnd_toEndOf="parent"
								app:layout_constraintStart_toEndOf="@+id/ImageView_idFeedbackNewIC"
								app:layout_constraintTop_toBottomOf="@+id/txtViewid_FeedbackNewIC"
								app:layout_constraintVertical_bias="0.218" />
						</androidx.constraintlayout.widget.ConstraintLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</ScrollView>
	</LinearLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
