<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Master|AnyCPU' ">
    <IntermediateOutputPath>obj\Release\netstandard2.0</IntermediateOutputPath>
    <Optimize>true</Optimize>
    <DefineConstants>TRACE;RELEASE;NETSTANDARD;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Refit" Version="4.6.58" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Data\Entities\" />
    <Folder Include="Business\Managers\" />
    <Folder Include="Resources\" />
    <Folder Include="Resources\Language\" />
    <Folder Include="Resources\Notification\" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Resources\Language\Language_EN.json" />
    <None Remove="Resources\Language\Language_MS.json" />
    <None Remove="Resources\Notification\GetUserNotifications.json" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\Language\Language_EN.json" />
    <EmbeddedResource Include="Resources\Language\Language_MS.json">
      <LogicalName>myTNB.Mobile.Resources.Language_MS.json</LogicalName>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\Notification\GetUserNotifications.json" />
  </ItemGroup>
</Project>
